
#wrapper {
    width: 100%;
    background: #202020;
}
.navbar-default{
    border:none;
}
.navbar-cls-top .navbar-brand {
    color: #fff;
    background: #202020;
    width: 260px;
    text-align: center;
    height: 92px;
    font-size: 20px;
    letter-spacing: 1px;
    font-weight: 900;
    padding-top: 35px;
    text-transform:uppercase;
}
.header-right {
    width:78.7%;
    background: #fff;
    float: right;
    font-size: 16px;
}
.header-right .btn{
    margin: 15px 50px 5px 0px;
    float: right;
}
.navbar-cls-top .navbar-brand:hover {
    background: #202020;
    color: #fff;
}

.user-img-div {
    min-height: 60px;
    background-color: #fff;
}
@media (min-width:768px) {
    #page-wrapper {
        margin: 0 0 0 260px;
        padding: 15px 30px;
        min-height: 1200px;
    }


    .navbar-side {
        z-index: 1;
        position: absolute;
        width: 260px;
    }

    .navbar {
        border-radius: 0px;
    }
}

.active-menu {
    background-color: #00CA79!important;
}
.sidebar-collapse > .nav > li > a {
    padding: 15px 10px;
}
.sidebar-collapse .nav > li > a {
    color: #fff;
    background: #202020;
    text-shadow: none;
    padding: 15px 40px;
    border-bottom: 1px solid #2F2E31;
    font-size:14px;
}
#main-menu > li > a+ul{
    display: none;
}
#main-menu > li > ul.active{
    display: block;
}
#main-menu > li > a:hover{
    background: #0A0A0A;
    outline: 0;
}
.sidebar-collapse .nav > li > ul a.active{
    background: #0A0A0A;
    outline: 0;
}
#main-menu > li > a.active{
    background: #0A0A0A;
    outline: 0;
}
.sidebar-collapse .nav > li > ul a{
    padding-left: 65px;
}
.header-right img {
    max-height: 92px;
    border:none;
    height: 92px;
    float: left;
}
.header-right span{
    font-size: 25px;
    line-height: 92px;
}
.user-img-div .inner-text {
    text-align: center;
    position: relative;
    color: #333;
    font-size:14px;
    font-weight: 800;
    line-height: 60px;
    width:100%;
    height:100%;
    float: right;
}
.fa.arrow:before {
    content: "\f104";
}
.arrow {
    float: right;
    transition:all 0.3s;
}
.active > a > .fa.arrow:before {
    content: "\f107";
}
.fa.arrow.active{
    transform: rotatez(-90deg);
}
iframe{
    width:78.8%;
    margin-left:260px;
    background: #f3f3f3;
    height:500px;
    border:0;
    padding:20px;
}
#footer-sec {
    background-color:#000;
    padding:20px 50px;
    color:#fff;
    font-size:12px;
    text-align: center;
}

#footer-sec a {
    color:#fff;
}
#footer-sec a:hover {
    color:#fff;
    text-decoration:none;
}